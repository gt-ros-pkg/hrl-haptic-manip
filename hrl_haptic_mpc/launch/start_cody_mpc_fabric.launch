<launch>

  <rosparam command="load" file="$(find hrl_haptic_mpc)/params/mpc_params_cody.yaml" /> 
  <rosparam command="load" file="$(find hrl_haptic_mpc)/params/cody_config_params.yaml" />

  <node name="mpc_teleop" pkg="hrl_haptic_mpc" type="mpc_teleop_rviz" args="-r cody " output="screen"/>
  <node name="waypoint_generator" pkg="hrl_haptic_mpc" type="waypoint_generator" args="-r cody -a l" output="screen"/>
  <node name="robot_haptic_state" pkg="hrl_haptic_mpc" type="robot_haptic_state_node" args="-r cody -s fabric -a l" output="screen"/>
  <node name="haptic_mpc" pkg="hrl_haptic_mpc" type="haptic_mpc" output="screen"/>


</launch>
